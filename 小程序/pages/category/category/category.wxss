page {
  background-color: var(--td-bg-color-container);
}

page .round-image {
  border-radius: 12rpx;
}

.t-search__input-container {
  border-radius: 32rpx !important;
  height: 64rpx !important;
  margin: 16rpx 32rpx;
}

.t-search__input {
  font-size: 28rpx !important;
  color: rgb(116, 116, 116) !important;
}

.side-bar-wrapper {
  display: flex;
  height: calc(100vh - 96rpx - var(--td-tab-bar-height, 80rpx) - env(safe-area-inset-bottom) - 32rpx);
}

.side-bar-wrapper .content {
  flex: 1;
}

.side-bar-wrapper .section {
  padding: 32rpx 0;
}

.side-bar-wrapper .title {
  padding-left: 40rpx;
  margin-bottom: 8rpx;
  line-height: 52rpx;
  color: var(--td-text-color-primary);
}

.side-bar-wrapper .image {
  width: 96rpx;
  height: 96rpx;
}